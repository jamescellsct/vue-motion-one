{
  "name": "v-motion",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./dist/v-motion.umd.js",
  "module": "./dist/v-motion.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-motion.es.js",
      "require": "./dist/v-motion.umd.js"
