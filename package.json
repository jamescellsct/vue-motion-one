    "build": "vue-tsc --noEmit && vite build",
    "build:demo": "vite build --mode demo"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "@vitejs/plugin-vue": "^1.9.0",
    "motion": "^10.0.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-dts": "^0.8.1",
    "vue-tsc": "^0.3.0",
    "vue": "^3.2.13"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "motion": "^10.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/v-motion.git"
  },
  "homepage": "https://github.com/wobsoriano/v-motion#readme"
}
